								<div class= "col-md-4">
									<%= property.label :zverdes, "Zonas Verdes Comunales"	%>
									<div class="verde horizontal toggle-modern" data-toggle-on="<%= @rural.property.zverdes.nil? ? false : !property.object.zverdes.to_i.zero? %>"></div>
									<%= property.check_box :zverdes, class: "pverde", hidden: true %>
								</div>

								<div class= "col-md-4">
									<%= property.label :chouse, "Club House"	%>
									<div class="house horizontal toggle-modern" data-toggle-on="<%= @rural.property.chouse.nil? ? false : !property.object.chouse.to_i.zero? %>"></div>
									<%= property.check_box :chouse, class: "phouse", hidden: true %>
								</div>
							</div>
						</div>
							
						<div class="row">
							<div class="row">
								<div class= "col-md-4">
									<%= property.label :ascensor, "Ascensor"	%>
									<div class="ascensor horizontal toggle-modern" data-toggle-on="<%= @rural.property.ascensor.nil? ? false : !property.object.ascensor.to_i.zero? %>"></div>
									<%= property.check_box :ascensor, class: "pascensor", hidden: true %>
								</div>
					
								
								<div class= "col-md-4">
									<%= property.label :gimnasio, "Gimnasio"	%>
									<div class="gym horizontal toggle-modern" data-toggle-on="<%= @rural.property.gimnasio.nil? ? false : !property.object.gimnasio.to_i.zero? %>"></div>
