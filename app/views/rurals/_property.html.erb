									<%= property.label :gvisitantes, "Garaje Visitantes"	%>
									<div class="visitan horizontal toggle-modern" data-toggle-on="<%= @rural.property.gvisitantes.nil? ? false : !property.object.gvisitantes.to_i.zero? %>"></div>
