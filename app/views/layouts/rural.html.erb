<!DOCTYPE html>
<html>
<head>
  <title>Avaluos</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= csrf_meta_tags %>
  <meta name="turbolinks-cache-control" content="no-cache">
</head>
<body data-no-turbolinks='true' class='fixed-header dashboard'>
	<!-- BEGIN SIDEBAR -->
  <div class="page-sidebar" data-pages="sidebar">
    <!-- BEGIN SIDEBAR HEADER -->
    <div class="sidebar-header">
  		<%= image_tag 'logo.png', alt: 'logo', width: '78', height: '22' %>
      <div class="sidebar-header-controls">
        <button data-pages-toggle="#appMenu" class="btn btn-xs sidebar-slide-toggle btn-link m-l-20" type="button"><i class="fa fa-angle-down fs-16"></i>
        </button>
        <button data-toggle-pin="sidebar" class="btn btn-link visible-lg-inline" type="button"><i class="fa fs-12"></i>
        </button>
      </div>
    </div>
    <!-- END SIDEBAR HEADER -->
    <!-- BEGIN SIDEBAR MENU -->
    <div class="sidebar-menu">
      <ul class="menu-items">
        <li class="m-t-30">
          <%= link_to "Lista Avalúos", rurals_path, class: 'detailed' %>
          <span class="icon-thumbnail"><i class="fa fa-home fa-fw"></i></span>
        </li>
      </ul>
    </div>
    <!-- END SIDEBAR MENU -->
  </div>
  <!-- END SIDEBAR -->
  <!-- START PAGE-CONTAINER -->
  <div class="page-container">
    <!-- START PAGE HEADER WRAPPER -->
    <!-- START HEADER -->
    <div class="header ">
      <!-- START MOBILE CONTROLS -->
      <div class="container-fluid relative">
            <!-- LEFT SIDE -->
            <div class="pull-left full-height visible-sm visible-xs">
              <!-- START ACTION BAR -->
              <div class="header-inner">
                <a href="#" class="btn-link toggle-sidebar visible-sm-inline-block visible-xs-inline-block padding-5" data-toggle="sidebar">
                  <span class="icon-set menu-hambuger"></span>
                </a>
              </div>
              <!-- END ACTION BAR -->
            </div>
            <div class="pull-center hidden-md hidden-lg">
              <div class="header-inner">
                <div class="brand inline">
                  <%= image_tag 'logo.png', alt: 'logo', width: '78', height: '22', :data => { :src => {:retina => "logo_2x.png" } } %>
              </div>
            </div>
            <!-- RIGHT SIDE -->
            <div class="pull-right full-height visible-sm visible-xs">
              <!-- START ACTION BAR -->
              <div class="header-inner">
                <a href="#" class="btn-link visible-sm-inline-block visible-xs-inline-block" data-toggle="quickview" data-toggle-element="#quickview">
                  <span class="icon-set menu-hambuger-plus"></span>
                </a>
              </div>
              <!-- END ACTION BAR -->
            </div>
          </div>
      <!-- LEFT SIDE -->
      <div class="pull-left full-height visible-sm visible-xs">
        <!-- START ACTION BAR -->
        <div class="sm-action-bar">
          <a href="#" class="btn-link toggle-sidebar" data-toggle="sidebar">
            <span class="icon-set menu-hambuger"></span>
          </a>
        </div>
        <!-- END ACTION BAR -->
      </div>
      <!-- RIGHT SIDE -->
      <div class="pull-right full-height visible-sm visible-xs">
        <!-- START ACTION BAR -->
        <div class="sm-action-bar">
          <a href="#" class="btn-link" data-toggle="quickview" data-toggle-element="#quickview">
            <span class="icon-set menu-hambuger-plus"></span>
          </a>
        </div>
        <!-- END ACTION BAR -->
      </div>
      <!-- END MOBILE CONTROLS -->
      <div class=" pull-left sm-table">
        <div class="header-inner">
          <div class="brand inline">
            <%= image_tag 'logo.png', alt: 'logo', width: '78', height: '22' %>
          </div>
        </div>
      </div>
      <div class=" pull-right">
        <div class="header-inner">
          <a href="#" class="btn-link icon-set menu-hambuger-plus m-l-20 sm-no-margin hidden-sm hidden-xs" data-toggle="quickview" data-toggle-element="#quickview"></a>
        </div>
      </div>
      <div class=" pull-right">
        <!-- START User Info-->
        <div class="visible-lg visible-md m-t-10">
          <div class="pull-left p-r-10 p-t-10 fs-16 font-heading">
            <span class="semi-bold"><%= current_user.email %></span>
            
          </div>
          <div class="dropdown pull-right">
            <button class="profile-dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              <span class="thumbnail-wrapper d32 circular inline m-t-5">
              <%= image_tag('green_house.jpg') %>
            
          </span>
            </button>
            <ul class="dropdown-menu profile-dropdown" role="menu">

              <li>
                <%= link_to edit_user_registration_path do %>
                  Cambiar Contrasena
                 <i class="fa fa-cog fa-fw"></i> 
                <% end %>

              </li>
              <li class="bg-master-lighter">
                <a href="/users/sign_out" data-method="delete"><i class="fa fa-power-off"></i>Cerrar sesión</a>     
              </li>
            </ul>
          </div>
        </div>
        <!-- END User Info-->
      </div>
    </div>
  </div>
<div class="page-content-wrapper">
  <!-- START PAGE CONTENT -->
  <div class="content">
    <!-- START JUMBOTRON -->
    <div class="jumbotron" data-pages="parallax">
      <div class="container-fluid container-fixed-lg sm-p-l-20 sm-p-r-20">
        <div class="inner">
          <!-- START BREADCRUMB -->
          <ul class="breadcrumb text-center">
            <li>Informe de avalúo</li>
          </ul>
          <!-- END BREADCRUMB -->
        </div>
      </div>
    </div>
    <!-- END JUMBOTRON -->
      <!-- START CONTAINER FLUID -->
      <div class="container-fluid container-fixed-lg">
      <div class="container-fluid container-fixed-lg bg-white">
        <!-- START PANEL -->
        <div class="panel panel-transparent">
          <%= yield %>
        </div>
            
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous">
	</script>

	<% javascript_include_tag 'pace.min' %>
	<% javascript_include_tag 'modernizr.custom' %>
	<% javascript_include_tag 'jquery.scrollbar.min' %>
	<% javascript_include_tag 'pages.min' %>
	<% javascript_include_tag 'toggles' %>
	<% javascript_include_tag 'toggles.min' %>

  <%= javascript_include_tag 'pages.min.js' %>
  
  <%= javascript_include_tag 'map.js' %>
</body>
</html>
